{
  "name": "kakerlake-react-electron",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "pnpm -r dev",
    "build": "pnpm -r build",
    "renderer:build": "pnpm -C apps/renderer build",
    "electron:publish": "pnpm -C apps/main publish"
  }
}